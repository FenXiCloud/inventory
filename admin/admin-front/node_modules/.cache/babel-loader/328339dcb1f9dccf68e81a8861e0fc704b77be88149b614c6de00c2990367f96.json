{"ast":null,"code":"import Role from \"@js/api/Role\";\nimport RoleForm from \"./RoleForm\";\nimport GrantMenu from \"./GrantMenu\";\nimport { confirm, message } from \"heyui.ext\";\nimport { layer } from \"@layui/layer-vue\";\nimport { h } from \"vue\";\n\n/**\r\n * @功能描述: 角色列表\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"RoleList\",\n  props: {\n    merchant: Object\n  },\n  data() {\n    return {\n      loading: false,\n      params: {\n        type: 0\n      },\n      checkedRows: [],\n      dataList: [],\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      }\n    };\n  },\n  computed: {\n    queryParams() {\n      return Object.assign(this.params, {\n        merchantId: this.merchant.id,\n        page: this.pagination.page,\n        pageSize: this.pagination.size\n      });\n    }\n  },\n  methods: {\n    showForm(entity) {\n      let type = 0;\n      let layerId = layer.open({\n        title: \"角色信息\",\n        shadeClose: false,\n        closeBtn: false,\n        area: ['400px', '230px'],\n        content: h(RoleForm, {\n          entity,\n          type,\n          merchant: this.merchant,\n          onClose: () => {\n            layer.close(layerId);\n          },\n          onSuccess: () => {\n            this.doSearch();\n            layer.close(layerId);\n          }\n        })\n      });\n    },\n    showGrantMenu(entity) {\n      let layerId = layer.drawer({\n        title: entity.name + \"-可用功能\",\n        area: ['40vw', '100vh'],\n        content: h(GrantMenu, {\n          entity,\n          merchant: this.merchant,\n          onClose: () => {\n            layer.close(layerId);\n          },\n          onSuccess: () => {\n            layer.close(layerId);\n          }\n        })\n      });\n    },\n    loadList() {\n      this.loading = true;\n      Role.list(this.queryParams).then(({\n        data\n      }) => {\n        this.dataList = data.results;\n        this.pagination.total = data.total;\n      }).finally(() => this.loading = false);\n    },\n    pageChange() {\n      this.loadList();\n    },\n    tableCheck() {\n      this.checkedRows = this.$refs.table.getCheckboxRecords();\n    },\n    doSearch() {\n      this.pagination.page = 1;\n      this.loadList();\n    },\n    doRemove(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认删除角色：${row.name}?`,\n        onConfirm: () => {\n          Role.remove(row.id).then(() => {\n            message(\"删除成功~\");\n            this.loadList();\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.loadList();\n  }\n};","map":{"version":3,"names":["Role","RoleForm","GrantMenu","confirm","message","layer","h","name","props","merchant","Object","data","loading","params","type","checkedRows","dataList","pagination","page","size","total","computed","queryParams","assign","merchantId","id","pageSize","methods","showForm","entity","layerId","open","title","shadeClose","closeBtn","area","content","onClose","close","onSuccess","doSearch","showGrantMenu","drawer","loadList","list","then","results","finally","pageChange","tableCheck","$refs","table","getCheckboxRecords","doRemove","row","onConfirm","remove","created"],"sources":["D:\\DevWorks\\jxc\\admin\\admin-front\\src\\components\\merchant\\role\\RoleList.vue"],"sourcesContent":["<template>\r\n  <div class=\"frame-page\" style=\"margin: 0\">\r\n    <div class=\"h-panel\">\r\n      <div class=\"h-panel-body\">\r\n        <div class=\"table-toolbar\">\r\n          <div class=\"table-toolbar-left\">\r\n          </div>\r\n          <div class=\"table-toolbar-right\">\r\n            <Button @click=\"showForm()\" color=\"primary\">添加</Button>\r\n          </div>\r\n        </div>\r\n        <vxe-table row-id=\"id\"\r\n                   ref=\"table\"\r\n                   :data=\"dataList\"\r\n                   highlight-hover-row\r\n                   :loading=\"loading\"\r\n                   :row-config=\"{height: 48}\"\r\n                   @checkbox-all=\"tableCheck\"\r\n                   @checkbox-change=\"tableCheck\">\r\n          <vxe-column type=\"seq\" width=\"60\" title=\"序列\" align=\"center\"/>\r\n          <vxe-column title=\"名称\" field=\"name\"/>\r\n          <vxe-column title=\"是否默认\" field=\"systemDefault\" width=\"100\" align=\"center\">\r\n            <template #default=\"{row:{systemDefault}}\">\r\n              <Tag color=\"primary\" v-if=\"systemDefault\">是</Tag>\r\n              <Tag color=\"red\" v-else>否</Tag>\r\n            </template>\r\n          </vxe-column>\r\n          <vxe-column title=\"操作\" align=\"center\" width=\"230\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"flex items-center justify-center\" v-if=\"!row.systemDefault\">\r\n                <span class=\"primary-color text-hover\" @click=\"showGrantMenu(row)\">可用功能</span>\r\n                <span class=\"primary-color text-hover ml-10px\" @click=\"showForm(row)\">编辑</span>\r\n                <span class=\"primary-color text-hover ml-10px\" @click=\"doRemove(row)\">删除</span>\r\n              </div>\r\n            </template>\r\n          </vxe-column>\r\n        </vxe-table>\r\n        <Pagination align=\"right\" class=\"mt-16px\" v-model=\"pagination\" @change=\"pageChange\" small/>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Role from \"@js/api/Role\";\r\nimport RoleForm from \"./RoleForm\";\r\nimport GrantMenu from \"./GrantMenu\";\r\nimport {confirm, message} from \"heyui.ext\";\r\nimport {layer} from \"@layui/layer-vue\";\r\nimport {h} from \"vue\";\r\n\r\n/**\r\n * @功能描述: 角色列表\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"RoleList\",\r\n  props: {\r\n    merchant: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      params: {\r\n        type: 0\r\n      },\r\n      checkedRows: [],\r\n      dataList: [],\r\n      pagination: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    queryParams() {\r\n      return Object.assign(this.params, {\r\n        merchantId: this.merchant.id,\r\n        page: this.pagination.page,\r\n        pageSize: this.pagination.size\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    showForm(entity) {\r\n      let type = 0;\r\n      let layerId = layer.open({\r\n        title: \"角色信息\",\r\n        shadeClose: false,\r\n        closeBtn: false,\r\n        area: ['400px', '230px'],\r\n        content: h(RoleForm, {\r\n          entity, type,\r\n          merchant: this.merchant,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    showGrantMenu(entity) {\r\n      let layerId = layer.drawer({\r\n        title: entity.name + \"-可用功能\",\r\n        area: ['40vw', '100vh'],\r\n        content: h(GrantMenu, {\r\n          entity,\r\n          merchant: this.merchant,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    loadList() {\r\n      this.loading = true;\r\n      Role.list(this.queryParams).then(({data}) => {\r\n        this.dataList = data.results;\r\n        this.pagination.total = data.total;\r\n      }).finally(() => this.loading = false);\r\n    },\r\n    pageChange() {\r\n      this.loadList();\r\n    },\r\n    tableCheck() {\r\n      this.checkedRows = this.$refs.table.getCheckboxRecords();\r\n    },\r\n    doSearch() {\r\n      this.pagination.page = 1;\r\n      this.loadList();\r\n    },\r\n    doRemove(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认删除角色：${row.name}?`,\r\n        onConfirm: () => {\r\n          Role.remove(row.id).then(() => {\r\n            message(\"删除成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.loadList();\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA4CA,OAAOA,IAAG,MAAO,cAAc;AAC/B,OAAOC,QAAO,MAAO,YAAY;AACjC,OAAOC,SAAQ,MAAO,aAAa;AACnC,SAAQC,OAAO,EAAEC,OAAO,QAAO,WAAW;AAC1C,SAAQC,KAAK,QAAO,kBAAkB;AACtC,SAAQC,CAAC,QAAO,KAAK;;AAErB;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,QAAQ,EAAEC;EACZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE;QACNC,IAAI,EAAE;MACR,CAAC;MACDC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;QACVC,IAAI,EAAE,CAAC;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT;IACF;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAOZ,MAAM,CAACa,MAAM,CAAC,IAAI,CAACV,MAAM,EAAE;QAChCW,UAAU,EAAE,IAAI,CAACf,QAAQ,CAACgB,EAAE;QAC5BP,IAAI,EAAE,IAAI,CAACD,UAAU,CAACC,IAAI;QAC1BQ,QAAQ,EAAE,IAAI,CAACT,UAAU,CAACE;MAC5B,CAAC;IACH;EACF,CAAC;EACDQ,OAAO,EAAE;IACPC,QAAQA,CAACC,MAAM,EAAE;MACf,IAAIf,IAAG,GAAI,CAAC;MACZ,IAAIgB,OAAM,GAAIzB,KAAK,CAAC0B,IAAI,CAAC;QACvBC,KAAK,EAAE,MAAM;QACbC,UAAU,EAAE,KAAK;QACjBC,QAAQ,EAAE,KAAK;QACfC,IAAI,EAAE,CAAC,OAAO,EAAE,OAAO,CAAC;QACxBC,OAAO,EAAE9B,CAAC,CAACL,QAAQ,EAAE;UACnB4B,MAAM;UAAEf,IAAI;UACZL,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvB4B,OAAO,EAAEA,CAAA,KAAM;YACbhC,KAAK,CAACiC,KAAK,CAACR,OAAO,CAAC;UACtB,CAAC;UACDS,SAAS,EAAEA,CAAA,KAAM;YACf,IAAI,CAACC,QAAQ,CAAC,CAAC;YACfnC,KAAK,CAACiC,KAAK,CAACR,OAAO,CAAC;UACtB;QACF,CAAC;MACH,CAAC,CAAC;IACJ,CAAC;IACDW,aAAaA,CAACZ,MAAM,EAAE;MACpB,IAAIC,OAAM,GAAIzB,KAAK,CAACqC,MAAM,CAAC;QACzBV,KAAK,EAAEH,MAAM,CAACtB,IAAG,GAAI,OAAO;QAC5B4B,IAAI,EAAE,CAAC,MAAM,EAAE,OAAO,CAAC;QACvBC,OAAO,EAAE9B,CAAC,CAACJ,SAAS,EAAE;UACpB2B,MAAM;UACNpB,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvB4B,OAAO,EAAEA,CAAA,KAAM;YACbhC,KAAK,CAACiC,KAAK,CAACR,OAAO,CAAC;UACtB,CAAC;UACDS,SAAS,EAAEA,CAAA,KAAM;YACflC,KAAK,CAA