{"ast":null,"code":"import AdminForm from \"./AdminForm.vue\";\nimport Admin from \"@js/api/Admin\";\nimport { confirm, message } from \"heyui.ext\";\nimport { layer } from \"@layui/layer-vue\";\nimport { h } from \"vue\";\n\n/**\r\n * @功能描述: 管理员列表\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"AdminList\",\n  components: {\n    AdminForm\n  },\n  props: {\n    merchant: Object\n  },\n  data() {\n    return {\n      loading: false,\n      params: {\n        name: null,\n        username: null,\n        phone: null\n      },\n      checkedRows: [],\n      dataList: [],\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      },\n      param: [{\n        title: '启用',\n        key: 'enabled'\n      }, {\n        title: '禁用',\n        key: 'disabled'\n      }]\n    };\n  },\n  computed: {\n    queryParams() {\n      return Object.assign(this.params, {\n        page: this.pagination.page,\n        pageSize: this.pagination.size\n      });\n    }\n  },\n  methods: {\n    showForm(entity) {\n      let layerId = layer.open({\n        title: \"管理员信息\",\n        shadeClose: false,\n        closeBtn: false,\n        area: ['500px', entity ? '370px' : '430px'],\n        content: h(AdminForm, {\n          entity,\n          merchant: this.merchant,\n          onClose: () => {\n            layer.close(layerId);\n          },\n          onSuccess: () => {\n            this.doSearch();\n            layer.close(layerId);\n          }\n        })\n      });\n    },\n    loadList() {\n      this.loading = true;\n      Admin.list(this.queryParams).then(({\n        data\n      }) => {\n        this.dataList = data.results;\n        this.pagination.total = data.total;\n      }).finally(() => this.loading = false);\n    },\n    pageChange() {\n      this.loadList();\n    },\n    tableCheck() {\n      this.checkedRows = this.$refs.table.getCheckboxRecords();\n    },\n    doSearch() {\n      this.pagination.page = 1;\n      this.loadList();\n    },\n    doRemove(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认删除管理员：${row.name}?`,\n        onConfirm: () => {\n          Admin.remove(row.id).then(() => {\n            message(\"删除成功~\");\n            this.loadList();\n          });\n        }\n      });\n    },\n    resetPassword(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认要重置【${row.name}】的登录密码?`,\n        onConfirm: () => {\n          Admin.resetPassword(row.id).then(() => {\n            message(\"重置成功~\");\n          });\n        }\n      });\n    },\n    trigger(code, row) {\n      let enabled = code === 'enabled';\n      confirm({\n        title: \"系统提示\",\n        content: `确认要「${enabled ? \"启用\" : \"禁用\"}」管理员：${row.name}?`,\n        onConfirm: () => {\n          Admin.save({\n            id: row.id,\n            enabled\n          }).then(() => {\n            message(\"操作成功~\");\n            this.loadList();\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.loadList();\n  }\n};","map":{"version":3,"names":["AdminForm","Admin","confirm","message","layer","h","name","components","props","merchant","Object","data","loading","params","username","phone","checkedRows","dataList","pagination","page","size","total","param","title","key","computed","queryParams","assign","pageSize","methods","showForm","entity","layerId","open","shadeClose","closeBtn","area","content","onClose","close","onSuccess","doSearch","loadList","list","then","results","finally","pageChange","tableCheck","$refs","table","getCheckboxRecords","doRemove","row","onConfirm","remove","id","resetPassword","trigger","code","enabled","save","created"],"sources":["D:\\DevWorks\\jxc\\admin\\admin-front\\src\\components\\merchant\\admin\\AdminList.vue"],"sourcesContent":["<template>\r\n  <div class=\"frame-page\" style=\"margin: 0\">\r\n    <div class=\"h-panel\">\r\n      <div class=\"h-panel-body\">\r\n        <div class=\"table-toolbar\">\r\n          <div class=\"table-toolbar-left\">\r\n            <label for=\"username\" class=\"mr-10px\">账号</label>\r\n            <Input id=\"username\" v-model=\"params.username\" class=\"flex-1\" placeholder=\"请输入账号\"/>\r\n            <label for=\"name\" class=\"mr-10px\">姓名</label>\r\n            <Input id=\"name\" v-model=\"params.name\" class=\"flex-1\" placeholder=\"请输入姓名\"/>\r\n            <label for=\"phone\" class=\"mr-10px\">电话</label>\r\n            <Input id=\"phone\" v-model=\"params.phone\" class=\"flex-1\" placeholder=\"请输入电话\"/>\r\n            <Button color=\"primary\" :loading=\"loading\" @click=\"doSearch\">查询</Button>\r\n          </div>\r\n          <div class=\"table-toolbar-right\">\r\n            <Button @click=\"showForm()\" color=\"primary\">添加</Button>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <vxe-table row-id=\"id\"\r\n                     ref=\"table\"\r\n                     :data=\"dataList\"\r\n                     highlight-hover-row\r\n                     show-overflow\r\n                     :row-config=\"{height: 48}\"\r\n                     :loading=\"loading\">\r\n            <vxe-column type=\"seq\" width=\"90\" title=\"序列\"/>\r\n            <vxe-column title=\"账号\" field=\"username\"/>\r\n            <vxe-column title=\"姓名\" field=\"name\"/>\r\n            <vxe-column title=\"电话\" field=\"phone\"/>\r\n            <vxe-column title=\"角色\" field=\"roleName\"/>\r\n            <vxe-column title=\"默认管理员\" field=\"systemDefault\">\r\n              <template #default=\"{row:{systemDefault}}\">\r\n                <Tag color=\"primary\" v-if=\"systemDefault\">是</Tag>\r\n                <Tag color=\"yellow\" v-else>否</Tag>\r\n              </template>\r\n            </vxe-column>\r\n            <vxe-column title=\"状态\" field=\"enabled\">\r\n              <template #default=\"{row:{enabled}}\">\r\n                <Tag color=\"primary\" v-if=\"enabled\">启用</Tag>\r\n                <Tag color=\"red\" v-else>禁用</Tag>\r\n              </template>\r\n            </vxe-column>\r\n            <vxe-column title=\"操作\" align=\"center\" width=\"200\">\r\n              <template #default=\"{row}\">\r\n                <div class=\"flex items-center justify-center\">\r\n                  <span class=\"primary-color text-hover \" @click=\"resetPassword(row)\" size=\"s\">重置密码</span>\r\n                  <span class=\" primary-color text-hover ml-10px\" @click=\"showForm(row)\" size=\"s\">编辑</span>\r\n                  <template v-if=\"!row.systemDefault\">\r\n                    <span class=\"primary-color ml-10px text-hover\" @click=\"doRemove(row)\" size=\"s\">删除</span>\r\n                    <DropdownMenu\r\n                        trigger=\"hover\"\r\n                        class=\"primary-color ml-8px  text-hover\"\r\n                        :datas=\"param\"\r\n                        @clickItem=\"trigger($event,row)\">\r\n                      更多\r\n                    </DropdownMenu>\r\n                  </template>\r\n                </div>\r\n              </template>\r\n            </vxe-column>\r\n          </vxe-table>\r\n          <Pagination align=\"right\" class=\"mt-16px\" v-model=\"pagination\" @change=\"pageChange\" small/>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AdminForm from \"./AdminForm.vue\";\r\nimport Admin from \"@js/api/Admin\";\r\nimport {confirm, message} from \"heyui.ext\";\r\nimport {layer} from \"@layui/layer-vue\";\r\nimport {h} from \"vue\";\r\n\r\n/**\r\n * @功能描述: 管理员列表\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"AdminList\",\r\n  components: {AdminForm},\r\n  props: {\r\n    merchant: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      params: {\r\n        name: null,\r\n        username: null,\r\n        phone: null,\r\n      },\r\n      checkedRows: [],\r\n      dataList: [],\r\n      pagination: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0\r\n      },\r\n      param: [\r\n        {title: '启用', key: 'enabled'},\r\n        {title: '禁用', key: 'disabled'},\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    queryParams() {\r\n      return Object.assign(this.params, {\r\n        page: this.pagination.page,\r\n        pageSize: this.pagination.size\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    showForm(entity) {\r\n      let layerId = layer.open({\r\n        title: \"管理员信息\",\r\n        shadeClose: false,\r\n        closeBtn: false,\r\n        area: ['500px', entity ? '370px' : '430px'],\r\n        content: h(AdminForm, {\r\n          entity,merchant:this.merchant,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    loadList() {\r\n      this.loading = true;\r\n      Admin.list(this.queryParams).then(({data}) => {\r\n        this.dataList = data.results;\r\n        this.pagination.total = data.total;\r\n      }).finally(() => this.loading = false);\r\n    },\r\n    pageChange() {\r\n      this.loadList();\r\n    },\r\n    tableCheck() {\r\n      this.checkedRows = this.$refs.table.getCheckboxRecords();\r\n    },\r\n    doSearch() {\r\n      this.pagination.page = 1;\r\n      this.loadList();\r\n    },\r\n    doRemove(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认删除管理员：${row.name}?`,\r\n        onConfirm: () => {\r\n          Admin.remove(row.id).then(() => {\r\n            message(\"删除成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    },\r\n    resetPassword(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认要重置【${row.name}】的登录密码?`,\r\n        onConfirm: () => {\r\n          Admin.resetPassword(row.id).then(() => {\r\n            message(\"重置成功~\");\r\n          })\r\n        }\r\n      })\r\n    },\r\n    trigger(code, row) {\r\n      let enabled = code === 'enabled';\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认要「${enabled ? \"启用\" : \"禁用\"}」管理员：${row.name}?`,\r\n        onConfirm: () => {\r\n          Admin.save({id: row.id, enabled}).then(() => {\r\n            message(\"操作成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.loadList();\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAsEA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,KAAI,MAAO,eAAe;AACjC,SAAQC,OAAO,EAAEC,OAAO,QAAO,WAAW;AAC1C,SAAQC,KAAK,QAAO,kBAAkB;AACtC,SAAQC,CAAC,QAAO,KAAK;;AAErB;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAACP;EAAS,CAAC;EACvBQ,KAAK,EAAE;IACLC,QAAQ,EAAEC;EACZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE;QACNP,IAAI,EAAE,IAAI;QACVQ,QAAQ,EAAE,IAAI;QACdC,KAAK,EAAE;MACT,CAAC;MACDC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;QACVC,IAAI,EAAE,CAAC;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAE,CACL;QAACC,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAS,CAAC,EAC7B;QAACD,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAU,CAAC;IAElC;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAOhB,MAAM,CAACiB,MAAM,CAAC,IAAI,CAACd,MAAM,EAAE;QAChCM,IAAI,EAAE,IAAI,CAACD,UAAU,CAACC,IAAI;QAC1BS,QAAQ,EAAE,IAAI,CAACV,UAAU,CAACE;MAC5B,CAAC;IACH;EACF,CAAC;EACDS,OAAO,EAAE;IACPC,QAAQA,CAACC,MAAM,EAAE;MACf,IAAIC,OAAM,GAAI5B,KAAK,CAAC6B,IAAI,CAAC;QACvBV,KAAK,EAAE,OAAO;QACdW,UAAU,EAAE,KAAK;QACjBC,QAAQ,EAAE,KAAK;QACfC,IAAI,EAAE,CAAC,OAAO,EAAEL,MAAK,GAAI,OAAM,GAAI,OAAO,CAAC;QAC3CM,OAAO,EAAEhC,CAAC,CAACL,SAAS,EAAE;UACpB+B,MAAM;UAACtB,QAAQ,EAAC,IAAI,CAACA,QAAQ;UAC7B6B,OAAO,EAAEA,CAAA,KAAM;YACblC,KAAK,CAACmC,KAAK,CAACP,OAAO,CAAC;UACtB,CAAC;UACDQ,SAAS,EAAEA,CAAA,KAAM;YACf,IAAI,CAACC,QAAQ,CAAC,CAAC;YACfrC,KAAK,CAACmC,KAAK,CAACP,OAAO,CAAC;UACtB;QACF