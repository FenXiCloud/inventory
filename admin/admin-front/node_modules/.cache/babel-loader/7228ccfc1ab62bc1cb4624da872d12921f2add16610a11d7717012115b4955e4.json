{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AppLogo from \"@components/app/AppLogo\";\nimport AppHead from \"@components/app/AppHead\";\nimport { mapState } from 'vuex';\n\n/**\r\n * @功能描述: 菜单\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"AppMenu\",\n  props: {\n    theme: String\n  },\n  data() {\n    return {};\n  },\n  watch: {\n    $route() {\n      this.menuSelect();\n    },\n    menus() {\n      this.menuSelect();\n    }\n  },\n  mounted() {\n    this.init();\n  },\n  computed: {\n    ...mapState(['siderCollapsed', 'menus']),\n    menuMode() {\n      return this.siderCollapsed ? 'collapse' : 'vertical';\n    }\n  },\n  methods: {\n    init() {\n      this.menuSelect();\n    },\n    menuSelect() {\n      if (this.$route.name) {\n        this.$refs.menu.select(this.$route.name);\n      }\n    },\n    trigger(data) {\n      if (data.children.length > 0) return;\n      this.$router.push({\n        name: data.key\n      });\n    },\n    hideMenu() {\n      this.$store.commit('updateSiderCollapse', true);\n    }\n  },\n  components: {\n    AppLogo,\n    AppHead\n  }\n};","map":{"version":3,"names":["AppLogo","AppHead","mapState","name","props","theme","String","data","watch","$route","menuSelect","menus","mounted","init","computed","menuMode","siderCollapsed","methods","$refs","menu","select","trigger","children","length","$router","push","key","hideMenu","$store","commit","components"],"sources":["D:\\DevWorks\\jxc\\admin\\admin-front\\src\\components\\app\\AppMenu.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-menu\">\r\n    <AppLogo/>\r\n    <Menu :datas=\"menus\" :mode=\"menuMode\" @clickItem=\"trigger\" ref=\"menu\" :className=\"`h-menu-${theme}`\"></Menu>\r\n    <!--    <div class=\"app-menu-mask\" @click=\"hideMenu\"></div>-->\r\n  </div>\r\n  <div>\r\n    <AppHead/>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\