{"ast":null,"code":"import Merchant from \"@js/api/Merchant\";\nimport { confirm, message } from \"heyui.ext\";\nimport MerchantForm from \"@components/merchant/MerchantForm\";\nimport MerchantSetting from \"@components/merchant/MerchantSetting\";\nimport { layer } from \"@layui/layer-vue\";\nimport { h } from \"vue\";\n\n/**\r\n * @功能描述: 商户列表\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"MerchantList\",\n  components: {\n    MerchantForm,\n    MerchantSetting\n  },\n  data() {\n    return {\n      merchant: null,\n      loading: false,\n      params: {\n        name: null\n      },\n      checkedRows: [],\n      dataList: [],\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      },\n      param: [{\n        title: '启用',\n        key: 'enabled'\n      }, {\n        title: '禁用',\n        key: 'disabled'\n      }]\n    };\n  },\n  computed: {\n    queryParams() {\n      return Object.assign(this.params, {\n        page: this.pagination.page,\n        pageSize: this.pagination.size\n      });\n    }\n  },\n  watch: {\n    'pagination.page'() {\n      this.pageChange();\n    }\n  },\n  methods: {\n    showForm(merchant) {\n      let layerId = layer.open({\n        title: \"商户信息\",\n        shadeClose: false,\n        closeBtn: false,\n        area: ['600px', 'auto'],\n        content: h(MerchantForm, {\n          merchant,\n          onClose: () => {\n            layer.close(layerId);\n          },\n          onSuccess: () => {\n            this.doSearch();\n            layer.close(layerId);\n          }\n        })\n      });\n    },\n    loadList() {\n      this.loading = true;\n      Merchant.list(this.queryParams).then(({\n        data\n      }) => {\n        this.dataList = data.results;\n        this.pagination.total = data.total;\n      }).finally(() => this.loading = false);\n    },\n    pageChange() {\n      this.loadList();\n    },\n    tableCheck() {\n      this.checkedRows = this.$refs.table.getCheckboxRecords();\n    },\n    doSearch() {\n      this.pagination.page = 1;\n      this.loadList();\n    },\n    doConfig(merchant) {\n      layer.drawer({\n        title: merchant.name,\n        area: ['80vw', '100vh'],\n        content: h(MerchantSetting, {\n          merchant\n        })\n      });\n    },\n    doRemove(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认删除商户：${row.name}?`,\n        onConfirm: () => {\n          Merchant.remove(row.id).then(() => {\n            message(\"删除成功~\");\n            this.loadList();\n          });\n        }\n      });\n    },\n    trigger(row) {\n      let enabled = !row.enabled;\n      confirm({\n        title: \"系统提示\",\n        content: `确认要「${enabled ? \"启用\" : \"禁用\"}」商户：${row.name}?`,\n        onConfirm: () => {\n          Merchant.save({\n            id: row.id,\n            enabled\n          }).then(() => {\n            message(\"操作成功~\");\n            this.loadList();\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.loadList();\n  }\n};","map":{"version":3,"names":["Merchant","confirm","message","MerchantForm","MerchantSetting","layer","h","name","components","data","merchant","loading","params","checkedRows","dataList","pagination","page","size","total","param","title","key","computed","queryParams","Object","assign","pageSize","watch","pagination.page","pageChange","methods","showForm","layerId","open","shadeClose","closeBtn","area","content","onClose","close","onSuccess","doSearch","loadList","list","then","results","finally","tableCheck","$refs","table","getCheckboxRecords","doConfig","drawer","doRemove","row","onConfirm","remove","id","trigger","enabled","save","created"],"sources":["D:\\DevWorks\\jxc\\admin\\admin-front\\src\\components\\merchant\\MerchantList.vue"],"sourcesContent":["<template>\r\n  <div class=\"frame-page\" style=\"margin: 0\">\r\n    <div class=\"h-panel\">\r\n      <div class=\"h-panel-body\">\r\n        <div class=\"table-toolbar\">\r\n          <div class=\"table-toolbar-left\">\r\n            <label for=\"name\" class=\"mr-10px\">商户名称</label>\r\n            <Input id=\"name\" v-model=\"params.name\" class=\"flex-1\" placeholder=\"请输入商户名称\"/>\r\n            <Button color=\"primary\" :loading=\"loading\" @click=\"doSearch\">查询</Button>\r\n          </div>\r\n          <div class=\"table-toolbar-right\">\r\n            <Button @click=\"showForm()\" color=\"primary\">添加</Button>\r\n          </div>\r\n        </div>\r\n        <vxe-table row-id=\"id\"\r\n                   ref=\"table\"\r\n                   :data=\"dataList\"\r\n                   highlight-hover-row\r\n                   show-overflow\r\n                   :row-config=\"{height: 48}\"\r\n                   :loading=\"loading\">\r\n          <vxe-column field=\"id\" width=\"60\" title=\"ID\"/>\r\n          <vxe-column title=\"商户编码\" field=\"code\" width=\"80\"/>\r\n          <vxe-column title=\"商户名称\" field=\"name\" width=\"220\">\r\n            <template #default=\"{row}\">\r\n              <span @click=\"doConfig(row)\" class=\"text-hover primary-color\"> <i class=\"h-icon-edit\"></i> {{ row.name }}</span>\r\n            </template>\r\n          </vxe-column>\r\n          <vxe-column title=\"联系人\" field=\"linkman\" width=\"200\"/>\r\n          <vxe-column title=\"联系人电话\" field=\"mobile\" width=\"130\"/>\r\n          <vxe-column title=\"服务时间\" field=\"serviceDate\" align=\"center\" width=\"200\">\r\n            <template #default=\"{row}\">\r\n              {{ row.serviceStartDate }} - {{ row.serviceEndDate }}\r\n            </template>\r\n          </vxe-column>\r\n          <vxe-column title=\"创建时间\" field=\"createDate\" align=\"center\" width=\"150\"/>\r\n          <vxe-column title=\"状态\" field=\"enabled\" align=\"center\" width=\"100\">\r\n            <template #default=\"{row}\">\r\n              <Tag color=\"primary\" v-if=\"row.enabled\" @click=\"trigger(row)\">启用</Tag>\r\n              <Tag color=\"red\" v-else @click=\"trigger(row)\">禁用</Tag>\r\n            </template>\r\n          </vxe-column>\r\n          <vxe-column title=\"操作\" align=\"center\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"flex items-center justify-center\">\r\n                <span class=\" primary-color text-hover ml-10px\" @click=\"showForm(row)\" size=\"s\">编辑</span>\r\n              </div>\r\n            </template>\r\n          </vxe-column>\r\n        </vxe-table>\r\n        <Pagination align=\"right\" class=\"mt-16px\" v-model=\"pagination\" @change=\"pageChange\" small/>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Merchant from \"@js/api/Merchant\";\r\nimport {confirm, message} from \"heyui.ext\";\r\nimport MerchantForm from \"@components/merchant/MerchantForm\";\r\nimport MerchantSetting from \"@components/merchant/MerchantSetting\";\r\nimport {layer} from \"@layui/layer-vue\";\r\nimport {h} from \"vue\";\r\n\r\n/**\r\n * @功能描述: 商户列表\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"MerchantList\",\r\n  components: {MerchantForm, MerchantSetting},\r\n  data() {\r\n    return {\r\n      merchant: null,\r\n      loading: false,\r\n      params: {\r\n        name: null\r\n      },\r\n      checkedRows: [],\r\n      dataList: [],\r\n      pagination: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0\r\n      },\r\n      param: [\r\n        {title: '启用', key: 'enabled'},\r\n        {title: '禁用', key: 'disabled'},\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    queryParams() {\r\n      return Object.assign(this.params, {\r\n        page: this.pagination.page,\r\n        pageSize: this.pagination.size\r\n      })\r\n    }\r\n  },\r\n  watch: {\r\n    'pagination.page'() {\r\n      this.pageChange();\r\n    }\r\n  },\r\n  methods: {\r\n    showForm(merchant) {\r\n      let layerId = layer.open({\r\n        title: \"商户信息\",\r\n        shadeClose: false,\r\n        closeBtn: false,\r\n        area: ['600px', 'auto'],\r\n        content: h(MerchantForm, {\r\n          merchant,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    }\r\n    ,\r\n    loadList() {\r\n      this.loading = true;\r\n      Merchant.list(this.queryParams).then(({data}) => {\r\n        this.dataList = data.results;\r\n        this.pagination.total = data.total;\r\n      }).finally(() => this.loading = false);\r\n    }\r\n    ,\r\n    pageChange() {\r\n      this.loadList();\r\n    }\r\n    ,\r\n    tableCheck() {\r\n      this.checkedRows = this.$refs.table.getCheckboxRecords();\r\n    }\r\n    ,\r\n    doSearch() {\r\n      this.pagination.page = 1;\r\n      this.loadList();\r\n    }\r\n    ,\r\n    doConfig(merchant) {\r\n      layer.drawer({\r\n        title: merchant.name,\r\n        area: ['80vw', '100vh'],\r\n        content: h(MerchantSetting, {\r\n          merchant\r\n        })\r\n      });\r\n    }\r\n    ,\r\n    doRemove(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认删除商户：${row.name}?`,\r\n        onConfirm: () => {\r\n          Merchant.remove(row.id).then(() => {\r\n            message(\"删除成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n    ,\r\n    trigger(row) {\r\n      let enabled = !row.enabled;\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认要「${enabled ? \"启用\" : \"禁用\"}」商户：${row.name}?`,\r\n        onConfirm: () => {\r\n          Merchant.save({id: row.id, enabled}).then(() => {\r\n            message(\"操作成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n  ,\r\n  created() {\r\n    this.loadList();\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAyDA,OAAOA,QAAO,MAAO,kBAAkB;AACvC,SAAQC,OAAO,EAAEC,OAAO,QAAO,WAAW;AAC1C,OAAOC,YAAW,MAAO,mCAAmC;AAC5D,OAAOC,eAAc,MAAO,sCAAsC;AAClE,SAAQC,KAAK,QAAO,kBAAkB;AACtC,SAAQC,CAAC,QAAO,KAAK;;AAErB;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IAACL,YAAY;IAAEC;EAAe,CAAC;EAC3CK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE;QACNL,IAAI,EAAE;MACR,CAAC;MACDM,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;QACVC,IAAI,EAAE,CAAC;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAE,CACL;QAACC,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAS,CAAC,EAC7B;QAACD,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAU,CAAC;IAElC;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAOC,MAAM,CAACC,MAAM,CAAC,IAAI,CAACb,MAAM,EAAE;QAChCI,IAAI,EAAE,IAAI,CAACD,UAAU,CAACC,IAAI;QAC1BU,QAAQ,EAAE,IAAI,CAACX,UAAU,CAACE;MAC5B,CAAC;IACH;EACF,CAAC;EACDU,KAAK,EAAE;IACL,iBAAiBC,CAAA,EAAG;MAClB,IAAI,CAACC,UAAU,CAAC,CAAC;IACnB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACrB,QAAQ,EAAE;MACjB,IAAIsB,OAAM,GAAI3B,KAAK,CAAC4B,IAAI,CAAC;QACvBb,KAAK,EAAE,MAAM;QACbc,UAAU,EAAE,KAAK;QACjBC,QAAQ,EAAE,KAAK;QACfC,IAAI,EAAE,CAAC,OAAO,EAAE,MAAM,CAAC;QACvBC,OAAO,EAAE/B,CAAC,CAACH,YAAY,EAAE;UACvBO,QAAQ;UACR4B,OAAO,EAAEA,CAAA,KAAM;YACbjC,KAAK,CAACkC,KAAK,CAACP,OAAO,CAAC;UACtB,CAAC;UACDQ,SAAS,EAAEA,CAAA,KAAM;YACf,IAAI,CAACC,QAAQ,CAAC,CAAC;YACfpC,KAAK,CAACkC,KAAK,CAACP,OAAO,CAAC;UACtB;QACF,CAAC;MACH,CAAC,CAAC;IACJ;IAEAU,QAAQA,CAAA,EAAG;MACT,IAAI,CAAC/B,OAAM,GAAI,IAAI;MACnBX,QAAQ,CAAC2C,IAAI,CAAC,IAAI,CAACpB,WAAW,CAAC,CAACqB,IAAI,CAAC,CAAC;QAACnC;MAAI,CAAC,KAAK;QAC/C,IAAI,CAACK,QAAO,GAAIL,IAAI,CAACoC,OAAO;QAC5B,IAAI,CAAC9B,UAAU,CAACG,KAAI,GAAIT,IAAI,CAACS,KAAK;MACpC,CAAC,CAAC,CAAC4B,OAAO,CAAC,MAAM,IAAI,CAACnC,OAAM,GAAI,KAAK,CAAC;IACxC;IAEAkB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACa,QAAQ,CAAC,CAAC;IACjB;IAEAK,UAAUA,CAAA,EAAG;MACX,IAAI,CAAClC,WAAU,GAAI,IAAI,CAACmC,KAAK,CAACC,KAAK,CAACC,kBAAkB,CAAC,CAAC;IAC1D;IAEAT,QAAQA,CAAA,EAAG;MACT,IAAI,CAAC1B,UAAU,CAACC,IAAG