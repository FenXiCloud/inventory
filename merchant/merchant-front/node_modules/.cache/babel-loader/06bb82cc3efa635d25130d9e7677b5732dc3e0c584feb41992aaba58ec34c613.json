 from \"./CustomerLevelForm.vue\";\r\nimport {confirm, message} from \"heyui.ext\";\r\nimport {layer} from \"@layui/layer-vue\";\r\nimport {h} from \"vue\";\r\n\r\n/**\r\n * @功能描述: 客户级别\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"CustomersLevelList\",\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      dataList: [],\r\n      params: {\r\n        filter: null,\r\n      },\r\n    }\r\n  },\r\n  methods: {\r\n    showForm(entity) {\r\n      let type = 0;\r\n      let layerId = layer.open({\r\n        title: \"客户等级\",\r\n        shadeClose: false,\r\n        closeBtn: false,\r\n        area: ['400px', '230px'],\r\n        content: h(CustomerLevelForm, {\r\n          entity, type,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    doSearch() {\r\n      this.loadList();\r\n    },\r\n    loadList() {\r\n      this.loading = true;\r\n      CustomerLevel.list(this.params).then(({data}) => {\r\n        this.dataList = data;\r\n      }).finally(() => this.loading = false);\r\n    },\r\n    doRemove(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认删除：${row.name}?`,\r\n        onConfirm: () => {\r\n          CustomerLevel.remove(row.id).then(() => {\r\n            message(\"删除成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.loadList();\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA0CA,OAAOA,aAAY,MAAO,uBAAuB;AACjD,OAAOC,iBAAgB,MAAO,yBAAyB;AACvD,SAAQC,OAAO,EAAEC,OAAO,QAAO,WAAW;AAC1C,SAAQC,KAAK,QAAO,kBAAkB;AACtC,SAAQC,CAAC,QAAO,KAAK;;AAErB;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,EAAE;MACZC,MAAM,EA