{"ast":null,"code":"import CustomerLevel from \"@js/api/CustomerLevel\";\nimport CustomerLevelForm from \"./CustomerLevelForm.vue\";\nimport { confirm, message } from \"heyui.ext\";\nimport { layer } from \"@layui/layer-vue\";\nimport { h } from \"vue\";\n\n/**\r\n * @功能描述: 客户级别\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"CustomersLevelList\",\n  data() {\n    return {\n      loading: false,\n      dataList: [],\n      params: {\n        filter: null\n      }\n    };\n  },\n  methods: {\n    showForm(entity) {\n      let type = 0;\n      let layerId = layer.open({\n        title: \"客户等级\",\n        shadeClose: false,\n        closeBtn: false,\n        area: ['400px', '230px'],\n        content: h(CustomerLevelForm, {\n          entity,\n          type,\n          onClose: () => {\n            layer.close(layerId);\n          },\n          onSuccess: () => {\n            this.doSearch();\n            layer.close(layerId);\n          }\n        })\n      });\n    },\n    doSearch() {\n      this.loadList();\n    },\n    loadList() {\n      this.loading = true;\n      CustomerLevel.list(this.params).then(({\n        data\n      }) => {\n        this.dataList = data;\n      }).finally(() => this.loading = false);\n    },\n    doRemove(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认删除：${row.name}?`,\n        onConfirm: () => {\n          CustomerLevel.remove(row.id).then(() => {\n            message(\"删除成功~\");\n            this.loadList();\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.loadList();\n  }\n};","map":{"version":3,"names":["CustomerLevel","CustomerLevelForm","confirm","message","layer","h","name","data","loading","dataList","params","filter","methods","showForm","entity","type","layerId","open","title","shadeClose","closeBtn","area","content","onClose","close","onSuccess","doSearch","loadList","li