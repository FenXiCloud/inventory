{"ast":null,"code":"import { mapState } from \"vuex\";\nimport { clone } from \"xe-utils\";\nimport { message } from \"heyui.ext\";\nimport Admin from \"@js/api/Admin\";\nexport default {\n  name: \"AccountBasic\",\n  computed: {\n    ...mapState(['user'])\n  },\n  data() {\n    return {\n      loading: false,\n      tab: 'base',\n      tabs: {\n        base: '基本信息',\n        safe: '安全设置'\n      },\n      admin: {},\n      passwordForm: {\n        oldPassword: null,\n        newPassword: null,\n        confirmPassword: null\n      },\n      validationRules: {\n        required: ['name', 'linkman', 'phone', 'oldPassword', 'newPassword', 'confirmPassword'],\n        mobile: ['phone']\n      }\n    };\n  },\n  methods: {\n    doSave() {\n      let validResult = this.$refs.form.valid();\n      if (validResult.result) {\n        this.loading = true;\n        Admin.save(this.admin).then(() => {\n          message(\"保存成功,重新登录后生效~\");\n        }).finally(() => this.loading = false);\n      }\n    },\n    doChange() {\n      let validResult = this.$refs.pform.valid();\n      if (validResult.result) {\n        this.loading = true;\n        Admin.updatePassword(this.passwordForm).then(() => {\n          message(\"保存成功,重新登录时生效~\");\n        }).finally(() => this.loading = false);\n      }\n    }\n  },\n  created() {\n    this.admin = clone(this.user.admin, true);\n  }\n};","map":{"version":3,"names":["mapState","clone","message","Admin","name","computed","data","loading","tab","tabs","base","safe","admin","passwordForm","oldPassword","newPassword","confirmPassword","validationRules","required","mobile","methods","doSave","validResult","$refs","form","valid","result","save","then","finally","doChange","pform","updatePassword","created","user"],"sources":["D:\\DevWorks\\jxc\\merchant\\merchant-front\\src\\views\\common\\AccountBasic.vue"],"sourcesContent":["<template>\r\n  <div class=\"frame-page\">\r\n    <div class=\"h-panel\">\r\n      <div class=\"h-panel-body\">\r\n        <Tabs :datas=\"tabs\" v-model=\"tab\"/>\r\n        <Form v-if=\"tab==='base'\" class=\"mt-16px w-400px\" mode=\"block\" ref=\"form\" :model=\"admin\" :rules=\"validationRules\">\r\n          <FormItem label=\"账号\">\r\n            <Input v-model=\"admin.username\" readonly/>\r\n          </FormItem>\r\n          <FormItem label=\"姓名\" prop=\"name\">\r\n            <Input v-model=\"admin.name\"/>\r\n          </FormItem>\r\n          <FormItem label=\"电话\" prop=\"phone\">\r\n            <Input v-model=\"admin.phone\"/>\r\n          </FormItem>\r\n          <FormItem>\r\n            <Button @click=\"doSave\" :loading=\"loading\" icon=\"fa fa-save\" color=\"primary\">保 存</Button>\r\n          </FormItem>\r\n        </Form>\r\n        <Form v-if=\"tab==='safe'\" class=\"mt-16px w-400px\" mode=\"block\" ref=\"pform\" :model=\"passwordForm\" :rules=\"validationRules\">\r\n          <FormItem label=\"原密码\" prop=\"oldPassword\">\r\n            <Input v-model=\"passwordForm.oldPassword\"/>\r\n          </FormItem>\r\n          <FormItem label=\"新密码\" prop=\"newPassword\">\r\n            <Input v-model=\"passwordForm.newPassword\"/>\r\n          </FormItem>\r\n          <FormItem label=\"确认新密码\" prop=\"confirmPassword\">\r\n            <Input v-model=\"passwordForm.confirmPassword\"/>\r\n          </FormItem>\r\n          <FormItem>\r\n            <Button @click=\"doChange\" :loading=\"loading\" icon=\"fa fa-save\" color=\"primary\">修 改 密 码</Button>\r\n          </FormItem>\r\n        </Form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {mapState} from \"vuex\"\r\nimport {clone} from \"xe-utils\"\r\nimport {message} from \"heyui.ext\";\r\nimport Admin from \"@js/api/Admin\";\r\n\r\nexport default {\r\n  name: \"AccountBasic\",\r\n  computed: {\r\n    ...mapState(['user'])\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      tab: 'base',\r\n      tabs: {\r\n        base: '基本信息',\r\n        safe: '安全设置',\r\n      },\r\n      admin: {},\r\n      passwordForm: {\r\n        oldPassword: null,\r\n        newPassword: null,\r\n        confirmPassword: null\r\n      },\r\n      validationRules: {\r\n        required: ['name', 'linkman', 'phone', 'oldPassword', 'newPassword', 'confirmPassword'],\r\n        mobile: ['phone']\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    doSave() {\r\n      let validResult = this.$refs.form.valid();\r\n      if (validResult.result) {\r\n        this.loading = true;\r\n        Admin.save(this.admin).then(() => {\r\n          message(\"保存成功,重新登录后生效~\");\r\n        }).finally(() => this.loading = false);\r\n      }\r\n    },\r\n    doChange() {\r\n      let validResult = this.$refs.pform.valid();\r\n      if (validResult.result) {\r\n        this.loading = true;\r\n        Admin.updatePassword(this.passwordForm).then(() => {\r\n          message(\"保存成功,重新登录时生效~\");\r\n        }).finally(() => this.loading = false);\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.admin = clone(this.user.admin, true);\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAwCA,SAAQA,QAAQ,QAAO,MAAK;AAC5B,SAAQC,KAAK,QAAO,UAAS;AAC7B,SAAQC,OAAO,QAAO,WAAW;AACjC,OAAOC,KAAI,MAAO,eAAe;AAEjC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,QAAQ,EAAE;IACR,GAAGL,QAAQ,CAAC,CAAC,MAAM,CAAC;EACtB,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,GAAG,EAAE,MAAM;MACXC,IAAI,EAAE;QACJC,IAAI,EAAE,MAAM;QACZC,IAAI,EAAE;MACR,CAAC;MACDC,KAAK,EAAE,CAAC,CAAC;MACTC,YAAY,EAAE;QACZC,WAAW,EAAE,IAAI;QACjBC,WAAW,EAAE,IAAI;QACjBC,eAAe,EAAE;MACnB,CAAC;MACDC,eAAe,EAAE;QACfC,QAAQ,EAAE,CAAC,MAAM,EAAE,SAAS,EAAE,OAAO,EAAE,aAAa,EAAE,aAAa,EAAE,iBAAiB,CAAC;QACvFC,MAAM,EAAE,CAAC,OAAO;MAClB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,IAAIC,WAAU,GAAI,IAAI,CAACC,KAAK,CAACC,IAAI,CAACC,KAAK,CAAC,CAAC;MACzC,IAAIH,WAAW,CAACI,MAAM,EAAE;QACtB,IAAI,CAACnB,OAAM,GAAI,IAAI;QACnBJ,KAAK,CAACwB,IAAI,CAAC,IAAI,CAACf,KAAK,CAAC,CAACgB,IAAI,CAAC,MAAM;UAChC1B,OAAO,CAAC,eAAe,CAAC;QAC1B,CAAC,CAAC,CAAC2B,OAAO,CAAC,MAAM,IAAI,CAAC