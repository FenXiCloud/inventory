createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_hoisted_3","_createVNode","_component_Select","modelValue","$data","selectAccountBookId","_cache","$event","datas","_ctx","accountBooks","onChange","$options","changeCurrent","deletable","user","admin","_hoisted_4","_component_DropdownMenu","className","trigger","offset","width","placement","infoMenu","onClickItem","default","_withCtx","_toDisplayString","name","_","_createCommentVNode"],"sources":["D:\\DevWorks\\jxc\\merchant\\merchant-front\\src\\views\\app\\AppHead.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-header\">\r\n    <div class=\"flex\">\r\n      <div class=\"account\">\r\n        <Select v-model=\"selectAccountBookId\" :datas=\"accountBooks\"  @change=\"changeCurrent\" :deletable=\"false\"></Select>\r\n      </div>\r\n    </div>\r\n    <div class=\"flex app-header-info flex items-center\" v-if=\"user.admin\">\r\n      <span class=\"h-icon-users mr-10px\"></span>\r\n      <DropdownMenu\r\n          className=\"app-header-dropdown\"\r\n          trigger=\"hover\"\r\n          offset=\"5,5\"\r\n          :width=\"120\"\r\n          placement=\"bottom-end\"\r\n          :datas=\"infoMenu\"\r\n          @clickItem=\"trigger\">\r\n        <span>{{ user.admin.name }}</span>\r\n      </DropdownMenu>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n/**\r\n * @功能描述: 头部\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nimport {onMounted, onUnmounted} from 'vue';\r\nimport {mapMutations, mapState} from 'vuex';\r\nimport {loading, confirm, message} from 'heyui.ext';\r\nimport { Logout} from \"@js/api/App\";\r\nimport AccountBook from \"@js/api/AccountBook\";\r\n\r\nexport default {\r\n  name: \"AppHead\",\r\n  data() {\r\n    return {\r\n      selectAccountBookId:null,\r\n      searchText: '',\r\n      infoMenu: [\r\n        {key: 'info', title: '个人信息', icon: 'h-icon-user'},\r\n        {key: 'logout', title: '退出登录', icon: 'h-icon-outbox'}\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(['user','checkout','accountBooks','accountBook']),\r\n    siderCollapsed: {\r\n      get() {\r\n        return this.$store.state.siderCollapsed;\r\n      },\r\n      set(value) {\r\n        this.$store.commit('updateSiderCollapse', value);\r\n      }\r\n    }\r\n  },\r\n  setup(props, context) {\r\n    let resizeEvent = null;\r\n    onMounted(() => {\r\n      let windowWidth = window.innerWidth;\r\n      resizeEvent = window.addEventListener('resize', () => {\r\n        if (windowWidth == window.innerWidth) {\r\n          return;\r\n        }\r\n        if (context.siderCollapsed && window.innerWidth > 900) {\r\n          context.siderCollapsed = false;\r\n        } else if (!context.siderCollapsed && window.innerWidth < 900) {\r\n          context.siderCollapsed = true;\r\n        }\r\n        windowWidth = window.innerWidth;\r\n      });\r\n      window.dispatchEvent(new Event('resize'));\r\n    });\r\n    onUnmounted(() => {\r\n      window.removeEventListener('resize', resizeEvent);\r\n    });\r\n  },\r\n  methods: {\r\n    ...mapMutations(['pushTab']),\r\n    changeCurrent(){\r\n      loading(\"正在切换，请稍后...\");\r\n      AccountBook.changeCurrent(this.selectAccountBookId).then(() => {\r\n        message('切换成功！');\r\n        localStorage.removeItem(\"currentTab\");\r\n        window.location.replace(\"/\")\r\n      });\r\n    },\r\n    trigger(data) {\r\n      if (data === 'logout') {\r\n        confirm({\r\n          title: \"系统提示\",\r\n          content: '确认退出？',\r\n          onConfirm: () => {\r\n            loading(\"登出中....\");\r\n            Logout().then(() => {\r\n              localStorage.removeItem(\"SYS_TABS\");\r\n              window.location.replace(\"/\")\r\n            }).finally(() => loading.close())\r\n          }\r\n        });\r\n      } else {\r\n        this.pushTab({key:'AccountBasic',title:'个人信息'})\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    if (this.accountBooks) {\r\n      this.selectAccountBookId = this.accountBook.key;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.app-header {\r\n  color: rgba(49, 58, 70, 0.8);\r\n  overflow: hidden;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  .account{\r\n    width: 250px;\r\n    margin-top: 5px;\r\n    margin-left: 10px;\r\n  }\r\n\r\n\r\n  .h-autocomplete {\r\n    line-height: 1.5;\r\n    float: left;\r\n    margin-top: 15px;\r\n    margin-right: 20px;\r\n    width: 120px;\r\n\r\n    &-show,\r\n    &-show:hover,\r\n    &-show.focusing {\r\n      outline: none;\r\n      box-shadow: none;\r\n      border-color: transparent;\r\n      border-radius: 0;\r\n    }\r\n\r\n    &-show.focusing {\r\n      border-bottom: 1px solid #eee;\r\n    }\r\n  }\r\n\r\n  &-info &-icon-item {\r\n    cursor: pointer;\r\n    display: inline-block;\r\n    float: left;\r\n    padding: 0 15px;\r\n    height: @layout-header-height;\r\n    line-height: @layout-header-height;\r\n    margin-right: 10px;\r\n\r\n    &:hover {\r\n      background: @hover-background-color;\r\n    }\r\n\r\n    i {\r\n      font-size: 18px;\r\n    }\r\n\r\n    a {\r\n      color: inherit;\r\n    }\r\n\r\n    .h-badge {\r\n      margin: 20px 0;\r\n      display: block;\r\n    }\r\n  }\r\n\r\n  .h-dropdownmenu {\r\n    float: left;\r\n  }\r\n\r\n  &-dropdown {\r\n    float: right;\r\n    margin-left: 10px;\r\n    padding: 0 20px 0 15px;\r\n\r\n    .h-icon-down {\r\n      right: 20px;\r\n    }\r\n\r\n    cursor: pointer;\r\n\r\n    &:hover,\r\n    &.h-pop-trigger {\r\n      background: @hover-background-color;\r\n    }\r\n\r\n    &-dropdown {\r\n      padding: 5px 0;\r\n\r\n      .h-dropdownmenu-item {\r\n        padding: 8px 20px;\r\n      }\r\n    }\r\n  }\r\n\r\n  &-menus {\r\n    display: inline-block;\r\n    vertical-align: top;\r\n\r\n    > div {\r\n      display: inline-block;\r\n      font-size: 15px;\r\n      padding: 0 25px;\r\n      color: @dark-color;\r\n\r\n      &:hover {\r\n        color: @primary-color;\r\n      }\r\n\r\n      + div {\r\n        margin-left: 5px;\r\n      }\r\n\r\n      &.h-tab-selected {\r\n        color: @white-color;\r\n        background-color: @primary-color;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.accountBook {\r\n  min-width: 150px;\r\n\r\n  li, dt {\r\n    cursor: pointer;\r\n    padding: 5px 10px;\r\n\r\n    &:hover {\r\n      background-color: @gray3-color;\r\n      color: @primary-color;\r\n    }\r\n  }\r\n\r\n  dt {\r\n    padding-left: 30px;\r\n  }\r\n}\r\n</style>\r\n\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAM;;EACVA,KAAK,EAAC;AAAS;;EAH1BC,GAAA;EAOSD,KAAK,EAAC;;;;;uBANbE,mBAAA,CAmBM,OAnBNC,UAmBM,GAlBJC,mBAAA,CAIM,OAJNC,UAIM,GAHJD,mBAAA,CAEM,OAFNE,UAEM,GADJC,YAAA,CAAiHC,iBAAA;IAJzHC,UAAA,EAIyBC,KAAA,CAAAC,mBAAmB;IAJ5C,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAIyBH,KAAA,CAAAC,mBAAmB,GAAAE,MAAA;IAAGC,KAAK,EAAEC,IAAA,CAAAC,YAAY;IAAIC,QAAM,EAAEC,QAAA,CAAAC,aAAa;IAAGC,SAAS,EAAE;oEAG3CL,IAAA,CAAAM,IAAI,CAACC,KAAK,I,cAApEpB,mBAAA,CAYM,OAZNqB,UAYM,G,0BAXJnB,mBAAA,CAA0C;IAApCJ,KAAK,EAAC;EAAsB,6BAClCO,YAAA,CASeiB,uBAAA;IARXC,SAAS,EAAC,qBAAqB;IAC/BC,OAAO,EAAC,OAAO;IACfC,MAAM,EAAC,KAAK;IACXC,KAAK,EAAE,GAAG;IACXC,SAAS,EAAC,YAAY;IACrBf,KAAK,EAAEJ,KAAA,CAAAoB,QAAQ;IACfC,WAAS,EAAEb,QAAA,CAAAQ;;IAhBtBM,OAAA,EAAAC,QAAA,CAiBQ,MAAkC,CAAlC7B,mBAAA,CAAkC,cAAA8B,gBAAA,CAAzBnB,IAAA,CAAAM,IAAI,CAACC,KAAK,CAACa,IAAI,iB;IAjBhCC,CAAA;mDAAAC,mBAAA,e","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}