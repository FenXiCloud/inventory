{"ast":null,"code":"import AppHead from \"@components/app/AppHead\";\nimport AppMenu from \"@components/app/AppMenu\";\nimport AppFooter from \"@components/app/AppFooter\";\nimport SysTabs from \"@components/common/sys-tabs\";\nimport { mapState } from \"vuex\";\nimport { message } from \"heyui.ext\";\n\n/**\r\n * @功能描述: FRAME\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"AppFrame\",\n  components: {\n    SysTabs,\n    AppFooter,\n    AppMenu,\n    AppHead\n  },\n  computed: {\n    ...mapState(['siderCollapsed', 'currentTab', 'tabs'])\n  },\n  errorCaptured(args) {\n    if (args.name === 'ChunkLoadError') {\n      alert('系统已更新，即将刷新页面...');\n      window.location.reload();\n      return false;\n    } else {\n      message.error(args.message);\n      return true;\n    }\n  }\n};","map":{"version":3,"names":["AppHead","AppMenu","AppFooter","SysTabs","mapState","message","name","components","computed","errorCaptured","args","alert","window","location","reload","error"],"sources":["D:\\DevWorks\\jxc\\merchant\\merchant-front\\src\\views\\app\\AppFrame.vue"],"sourcesContent":["<template>\r\n  <Layout class=\"app-frame\" :siderCollapsed=\"siderCollapsed\" siderFixed>\r\n    <Sider theme=\"dark\" style=\"overflow: unset;!important;\">\r\n      <AppMenu theme=\"dark\"/>\r\n    </Sider>\r\n    <Layout headerFixed>\r\n      <HHeader theme=\"white\">\r\n        <AppHead/>\r\n      </HHeader>\r\n      <SysTabs :homePage=\"currentTab\"/>\r\n      <Content>\r\n        <div class=\"app-frame-content h-full p-10px pt-10px\">\r\n          <Suspense>\r\n            <component is=\"DashboardMain\" v-show=\"'DashboardMain'===currentTab\"/>\r\n            <template #fallback>\r\n              <div class=\"bg-white-color h-full flex justify-center items-center flex-column\">\r\n                <div class=\"mb-16px\">\r\n                  <div class=\"loading\" data-v-79c34abf=\"\">\r\n                    <div data-v-79c34abf=\"\"></div>\r\n                    <div data-v-79c34abf=\"\"></div>\r\n                    <div data-v-79c34abf=\"\"></div>\r\n                    <div data-v-79c34abf=\"\"></div>\r\n                    <div data-v-79c34abf=\"\"></div>\r\n                  </div>\r\n                </div>\r\n                <div>页面加载中,请稍后...</div>\r\n              </div>\r\n            </template>\r\n          </Suspense>\r\n          <template v-for=\"tab in tabs\">\r\n            <Suspense>\r\n              <component class=\"h-full bg-white-color\" v-show=\"tab.key===currentTab\" :is=\"tab.key\" :pro=\"tab.params\"/>\r\n              <!-- 加载中状态 -->\r\n              <template #fallback>\r\n                <div class=\"bg-white-color h-full flex justify-center items-center flex-column\">\r\n                  <div class=\"mb-16px\">\r\n                    <div class=\"loading\" data-v-79c34abf=\"\">\r\n                      <div data-v-79c34abf=\"\"></div>\r\n                      <div data-v-79c34abf=\"\"></div>\r\n                      <div data-v-79c34abf=\"\"></div>\r\n                      <div data-v-79c34abf=\"\"></div>\r\n                      <div data-v-79c34abf=\"\"></div>\r\n                    </div>\r\n                  </div>\r\n                  <div>页面加载中,请稍后...</div>\r\n                </div>\r\n              </template>\r\n            </Suspense>\r\n          </template>\r\n        </div>\r\n        <HFooter>\r\n          <AppFooter/>\r\n        </HFooter>\r\n      </Content>\r\n    </Layout>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport AppHead from \"@components/app/AppHead\";\r\nimport AppMenu from \"@components/app/AppMenu\";\r\nimport AppFooter from \"@components/app/AppFooter\";\r\nimport SysTabs from \"@components/common/sys-tabs\";\r\nimport {mapState} from \"vuex\";\r\nimport {message} from \"heyui.ext\";\r\n\r\n/**\r\n * @功能描述: FRAME\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"AppFrame\",\r\n  components: {SysTabs, AppFooter, AppMenu, AppHead},\r\n  computed: {\r\n    ...mapState(['siderCollapsed', 'currentTab', 'tabs'])\r\n  },\r\n  errorCaptured(args) {\r\n    if (args.name === 'ChunkLoadError') {\r\n      alert('系统已更新，即将刷新页面...');\r\n      window.location.reload();\r\n      return false;\r\n    } else {\r\n      message.error(args.message);\r\n      return true;\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA2DA,OAAOA,OAAM,MAAO,yBAAyB;AAC7C,OAAOC,OAAM,MAAO,yBAAyB;AAC7C,OAAOC,SAAQ,MAAO,2BAA2B;AACjD,OAAOC,OAAM,MAAO,6BAA6B;AACjD,SAAQC,QAAQ,QAAO,MAAM;AAC7B,SAAQC,OAAO,QAAO,WAAW;;AAEjC;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAACJ,OAAO;IAAED,SAAS;IAAED,OAAO;IAAED;EAAO,CAAC;EAClDQ,QAAQ,EAAE;IACR,GAAGJ,QAAQ,CAAC,CAAC,gBAAgB,EAAE,YAAY,EAAE,MAAM,CAAC;EACtD,CAAC;EACDK,aAAaA,CAACC,IAAI,EAAE;IAClB,IAAIA,IAAI,CAACJ,IAAG,KAAM,gBAAgB,EAAE;MAClCK,KAAK,CAAC,iBAAiB,CAAC;MACxBC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC,CAAC;MACxB,OAAO,KAAK;IACd,OAAO;MACLT,OAAO,CAACU,KAAK,CAACL,IAAI,CAACL,OAAO,CAAC;MAC3B,OAAO,IAAI;IACb;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}