{"ast":null,"code":"import AccountBook from \"@js/api/AccountBook\";\nimport { confirm, message } from \"heyui.ext\";\nimport AccountBookForm from \"./AccountBookForm.vue\";\nimport { layer } from \"@layui/layer-vue\";\nimport { h } from \"vue\";\nexport default {\n  name: \"AccountBookList\",\n  props: {\n    merchant: Object\n  },\n  components: {\n    AccountBookForm\n  },\n  data() {\n    return {\n      opened: true,\n      loading: false,\n      params: {\n        areaId: null,\n        name: null,\n        merchantId: null\n      },\n      checkedRows: [],\n      dataList: [],\n      areaList: [],\n      merchantList: [],\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      },\n      param: [{\n        title: '启用',\n        key: 'enabled'\n      }, {\n        title: '禁用',\n        key: 'disabled'\n      }]\n    };\n  },\n  computed: {\n    queryParams() {\n      return Object.assign(this.params, {\n        page: this.pagination.page,\n        pageSize: this.pagination.size\n      });\n    }\n  },\n  methods: {\n    showForm(accountBook) {\n      let layerId = layer.open({\n        title: \"组织信息\",\n        shadeClose: false,\n        area: ['50vw', 'auto'],\n        content: h(AccountBookForm, {\n          accountBook,\n          onClose: () => {\n            layer.close(layerId);\n          },\n          onSuccess: () => {\n            this.doSearch();\n            layer.close(layerId);\n          }\n        })\n      });\n    },\n    loadList() {\n      this.loading = true;\n      AccountBook.list(this.queryParams).then(({\n        data\n      }) => {\n        this.dataList = data.results;\n        this.pagination.total = data.total;\n      }).finally(() => this.loading = false);\n    },\n    pageChange() {\n      this.loadList();\n    },\n    doSearch() {\n      this.pagination.page = 1;\n      this.loadList();\n    },\n    doRemove(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认删除账套：${row.name}?`,\n        onConfirm: () => {\n          AccountBook.remove(row.id).then(() => {\n            message(\"删除成功~\");\n            this.doSearch();\n          });\n        }\n      });\n    },\n    trigger(row) {\n      let enabled = !row.enabled;\n      confirm({\n        title: \"系统提示\",\n        content: `确认要「${enabled ? \"启用\" : \"禁用\"}」账套：${row.name}?`,\n        onConfirm: () => {\n          AccountBook.save({\n            id: row.id,\n            enabled\n          }).then(() => {\n            message(\"操作成功~\");\n            this.loadList();\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.doSearch();\n  }\n};","map":{"version":3,"names":["AccountBook","confirm","message","AccountBookForm","layer","h","name","props","merchant","Object","components","data","opened","loading","params","areaId","merchantId","checkedRows","dataList","areaList","merchantList","pagination","page","size","total","param","title","key","computed","queryParams","assign","pageSize","methods","showForm","accountBook","layerId","open","shadeClose","area","content","onClose","close","onSuccess","doSearch","loadList","list","then","results","finally","pageChange","doRemove","row","onConfirm","remove","id","trigger","enabled","save","created"],"sources":["D:\\DevWorks\\jxc\\merchant\\merchant-front\\src\\views\\setting\\AccountBookList.vue"],"sourcesContent":["<template>\r\n    <div class=\"frame-page flex flex-column\">\r\n      <vxe-toolbar>\r\n        <template #buttons>\r\n          <Search v-model.trim=\"params.name\" search-button-theme=\"h-btn-default\"\r\n                  show-search-button class=\"w-360px\"\r\n                  placeholder=\"请输入名称\" @search=\"doSearch\">\r\n            <i class=\"h-icon-search\"/>\r\n          </Search>\r\n        </template>\r\n        <template #tools>\r\n          <Button @click=\"showForm()\" color=\"primary\">新 增</Button>\r\n        </template>\r\n      </vxe-toolbar>\r\n      <div class=\"flex1\">\r\n        <vxe-table row-id=\"id\"\r\n                   ref=\"table\"\r\n                   :data=\"dataList\"\r\n                   highlight-hover-row\r\n                   show-overflow\r\n                   :loading=\"loading\">\r\n          <vxe-column type=\"seq\" width=\"60\" align=\"center\"/>\r\n          <vxe-column title=\"名称\" field=\"name\" min-width=\"150\"/>\r\n          <vxe-column title=\"启用日期\" field=\"startDate\" width=\"120\" formatter=\"formatMonth\"/>\r\n          <vxe-column title=\"状态\" field=\"enabled\" width=\"80\" align=\"center\" >\r\n            <template #default=\"{row}\">\r\n              <Tag color=\"primary\" v-if=\"row.enabled\" @click=\"trigger(row)\" class=\"cursor-pointer\">启用</Tag>\r\n              <Tag color=\"red\" v-else @click=\"trigger(row)\" class=\"cursor-pointer\">禁用</Tag>\r\n            </template>\r\n          </vxe-column>\r\n          <vxe-column title=\"操作\" align=\"center\" width=\"120\" fixed=\"right\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"flex items-center justify-center\">\r\n                <span class=\" primary-color text-hover ml-10px\" @click=\"showForm(row)\" size=\"s\">编辑</span>\r\n              </div>\r\n            </template>\r\n          </vxe-column>\r\n        </vxe-table>\r\n        <div class=\"flex justify-between items-center pt-5px\">\r\n          <div></div>\r\n          <vxe-pager perfect @page-change=\"loadList()\"\r\n                     v-model:current-page=\"pagination.page\"\r\n                     v-model:page-size=\"pagination.pageSize\"\r\n                     :total=\"pagination.total\"\r\n                     :layouts=\"['PrevJump', 'PrevPage', 'Number', 'NextPage', 'NextJump', 'Sizes', 'FullJump', 'Total']\">\r\n            <template #left>\r\n              <vxe-button @click=\"loadList(false)\" type=\"text\" size=\"mini\" icon=\"fa fa-refresh\"\r\n                          :loading=\"loading\"></vxe-button>\r\n            </template>\r\n          </vxe-pager>\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AccountBook from \"@js/api/AccountBook\";\r\nimport {confirm, message} from \"heyui.ext\";\r\nimport AccountBookForm from \"./AccountBookForm.vue\";\r\nimport {layer} from \"@layui/layer-vue\";\r\nimport {h} from \"vue\";\r\n\r\n\r\nexport default {\r\n  name: \"AccountBookList\",\r\n  props: {\r\n    merchant: Object,\r\n  },\r\n  components: {AccountBookForm},\r\n  data() {\r\n    return {\r\n      opened: true,\r\n      loading: false,\r\n      params: {\r\n        areaId: null,\r\n        name: null,\r\n        merchantId: null,\r\n      },\r\n      checkedRows: [],\r\n      dataList: [],\r\n      areaList: [],\r\n      merchantList: [],\r\n      pagination: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0\r\n      },\r\n      param: [\r\n        {title: '启用', key: 'enabled'},\r\n        {title: '禁用', key: 'disabled'},\r\n      ]\r\n\r\n    }\r\n  },\r\n  computed: {\r\n    queryParams() {\r\n      return Object.assign(this.params, {\r\n        page: this.pagination.page,\r\n        pageSize: this.pagination.size\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    showForm(accountBook) {\r\n      let layerId = layer.open({\r\n        title: \"组织信息\",\r\n        shadeClose: false,\r\n        area: ['50vw', 'auto'],\r\n        content: h(AccountBookForm, {\r\n          accountBook,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    loadList() {\r\n      this.loading = true;\r\n      AccountBook.list(this.queryParams).then(({data}) => {\r\n        this.dataList = data.results;\r\n        this.pagination.total = data.total;\r\n      }).finally(() => this.loading = false);\r\n    },\r\n    pageChange() {\r\n      this.loadList();\r\n    },\r\n    doSearch() {\r\n      this.pagination.page = 1;\r\n      this.loadList();\r\n    },\r\n    doRemove(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认删除账套：${row.name}?`,\r\n        onConfirm: () => {\r\n          AccountBook.remove(row.id).then(() => {\r\n            message(\"删除成功~\");\r\n            this.doSearch();\r\n          })\r\n        }\r\n      })\r\n    },\r\n    trigger(row) {\r\n      let enabled = !row.enabled;\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认要「${enabled ? \"启用\" : \"禁用\"}」账套：${row.name}?`,\r\n        onConfirm: () => {\r\n          AccountBook.save({id: row.id, enabled}).then(() => {\r\n            message(\"操作成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.doSearch();\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAwDA,OAAOA,WAAU,MAAO,qBAAqB;AAC7C,SAAQC,OAAO,EAAEC,OAAO,QAAO,WAAW;AAC1C,OAAOC,eAAc,MAAO,uBAAuB;AACnD,SAAQC,KAAK,QAAO,kBAAkB;AACtC,SAAQC,CAAC,QAAO,KAAK;AAGrB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE;IACLC,QAAQ,EAAEC;EACZ,CAAC;EACDC,UAAU,EAAE;IAACP;EAAe,CAAC;EAC7BQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE;QACNC,MAAM,EAAE,IAAI;QACZT,IAAI,EAAE,IAAI;QACVU,UAAU,EAAE;MACd,CAAC;MACDC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAE;MAChBC,UAAU,EAAE;QACVC,IAAI,EAAE,CAAC;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAE,CACL;QAACC,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAS,CAAC,EAC7B;QAACD,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAU,CAAC;IAGlC;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAOpB,MAAM,CAACqB,MAAM,CAAC,IAAI,CAAChB,MAAM,EAAE;QAChCQ,IAAI,EAAE,IAAI,CAACD,UAAU,CAACC,IAAI;QAC1BS,QAAQ,EAAE,IAAI,CAACV,UAAU,CAACE;MAC5B,CAAC;IACH;EACF,CAAC;EACDS,OAAO,EAAE;IACPC,QAAQA,CAACC,WAAW,EAAE;MACpB,IAAIC,OAAM,GAAI/B,KAAK,CAACgC,IAAI,CAAC;QACvBV,KAAK,EAAE,MAAM;QACbW,UAAU,EAAE,KAAK;QACjBC,IAAI,EAAE,CAAC,MAAM,EAAE,MAAM,CAAC;QACtBC,OAAO,EAAElC,CAAC,CAACF,eAAe,EAAE;UAC1B+B,WAAW;UACXM