{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport AppLogo from \"@views/app/AppLogo\";\nimport { mapState, mapMutations } from 'vuex';\nimport { pick } from \"xe-utils\";\n\n/**\r\n * @功能描述: 菜单\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\nexport default {\n  name: \"AppMenu\",\n  props: {\n    theme: String\n  },\n  computed: {\n    ...mapState(['siderCollapsed', 'menus', \"currentTab\", 'tabs']),\n    menuMode() {\n      return this.siderCollapsed ? 'collapse' : 'vertical';\n    }\n  },\n  watch: {\n    currentTab(val) {\n      location.hash = val;\n      localStorage.setItem(\"currentTab\", JSON.stringify(pick(this.tabs.find(value => value.key === val), ['key', 'title'])));\n    }\n  },\n  methods: {\n    ...mapMutations(['pushTab', 'updateTab']),\n    selected(item) {\n      if (\"DashboardMain\" !== this.currentTab) {\n        if (this.menus) {\n          let find = this.menus.find(val => {\n            if (val.key === this.currentTab) {\n              return true;\n            } else if (val.children) {\n              for (const c1 of val.children) {\n                if (c1.key === this.currentTab) {\n                  return true;\n                } else if (c1.children) {\n                  for (const c2 of c1.children) {\n                    if (c2.key === this.currentTab) {\n                      return true;\n                    }\n                  }\n                }\n              }\n            }\n          });\n          return find && item.key === find.key;\n        }\n      }\n      return false;\n    },\n    trigger(data, level) {\n      if (data.key === 'DashboardMain') {\n        this.updateTab('DashboardMain');\n      } else if (data.key && level === 2) {\n        this.pushTab(data);\n      } else {\n        if (!data.children) {\n          this.pushTab(data);\n        }\n      }\n    },\n    hideMenu() {\n      this.$store.commit('updateSiderCollapse', true);\n    }\n  },\n  components: {\n    AppLogo\n  }\n};","map":{"version":3,"names":["AppLogo","mapState","mapMutations","pick","name","props","theme","String","computed","menuMode","siderCollapsed","watch","currentTab","val","location","hash","localStorage","setItem","JSON","stringify","tabs","find","value","key","methods","selected","item","menus","children","c1","c2","trigger","data","level","updateTab","pushTab","hideMenu","$store","commit","components"],"sources":["D:\\DevWorks\\jxc\\merchant\\merchant-front\\src\\views\\app\\AppMenu.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-menu\">\r\n    <AppLogo/>\r\n    <div class=\"h-menu h-menu-dark\" :class=\"siderCollapsed?'h-menu-mode-collapse':'h-menu-mode-vertical'\">\r\n      <li class=\"h-menu-li h-menu-first-level\" v-for=\"m in menus\" @click.stop=\"trigger(m,1)\">\r\n        <Tooltip v-if=\"siderCollapsed&& m.children &&!m.children.length\" :content=\"m.title\" placement=\"right\">\r\n          <div class=\"h-menu-show\" :class=\"{'h-menu-li-selected':selected(m)}\">\r\n          <span class=\"h-menu-show-icon\" v-if=\"m.icon\">\r\n            <i :class=\"m.icon\"></i>\r\n          </span>\r\n            <span class=\"h-menu-show-desc text-16px\">{{ m.title }}</span>\r\n          </div>\r\n        </Tooltip>\r\n        <template v-else>\r\n          <div class=\"h-menu-show\" :class=\"{'h-menu-li-selected':selected(m)}\">\r\n          <span class=\"h-menu-show-icon\" v-if=\"m.icon\">\r\n            <i :class=\"m.icon\"></i>\r\n          </span>\r\n            <span class=\"h-menu-show-desc text-16px\">{{ m.title }}</span>\r\n          </div>\r\n        </template>\r\n        <div class=\"second-menu\">\r\n          <div class=\"second-menu-wrap\">\r\n            <ul class=\"second-menu-item\" v-for=\"c1 in m.children\" :key=\"c1.key\">\r\n              <span class=\"second-menu-title text-14px\">{{ c1.title }}</span>\r\n              <li class=\"text-14px\" v-for=\"c2 in c1.children\" @click.stop=\"trigger(c2,2)\" :key=\"c2.key\">{{ c2.title }}</li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </li>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppLogo from \"@views/app/AppLogo\";\r\nimport {mapState,mapMutations} from 'vuex';\r\nimport {pick} from \"xe-utils\";\r\n\r\n/**\r\n * @功能描述: 菜单\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"AppMenu\",\r\n  props: {\r\n    theme: String\r\n  },\r\n  computed: {\r\n    ...mapState(['siderCollapsed', 'menus', \"currentTab\", 'tabs']),\r\n    menuMode() {\r\n      return this.siderCollapsed ? 'collapse' : 'vertical';\r\n    }\r\n  },\r\n  watch: {\r\n    currentTab(val) {\r\n      location.hash = val;\r\n      localStorage.setItem(\"currentTab\", JSON.stringify(pick(this.tabs.find(value => value.key === val), ['key', 'title'])))\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapMutations(['pushTab', 'updateTab']),\r\n    selected(item) {\r\n      if (\"DashboardMain\" !== this.currentTab) {\r\n        if (this.menus) {\r\n          let find = this.menus.find(val => {\r\n            if (val.key === this.currentTab) {\r\n              return true;\r\n            } else if (val.children) {\r\n              for (const c1 of val.children) {\r\n                if (c1.key === this.currentTab) {\r\n                  return true;\r\n                } else if (c1.children) {\r\n                  for (const c2 of c1.children) {\r\n                    if (c2.key === this.currentTab) {\r\n                      return true;\r\n                    }\r\n                  }\r\n                }\r\n              }\r\n            }\r\n          })\r\n          return find && (item.key === find.key);\r\n        }\r\n      }\r\n      return false;\r\n    },\r\n    trigger(data, level) {\r\n      if (data.key === 'DashboardMain') {\r\n        this.updateTab('DashboardMain');\r\n      } else if (data.key && level === 2) {\r\n        this.pushTab(data);\r\n      } else {\r\n        if (!data.children){\r\n          this.pushTab(data);\r\n        }\r\n      }\r\n    },\r\n    hideMenu() {\r\n      this.$store.commit('updateSiderCollapse', true);\r\n    }\r\n  },\r\n  components: {\r\n    AppLogo\r\n  }\r\n}\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.h-menu-mode-vertical {\r\n  > .h-menu-show {\r\n    padding-left: 15px;\r\n    position: relative;\r\n  }\r\n\r\n\r\n  .h-menu-show {\r\n    overflow: hidden;\r\n    justify-content: left;\r\n    text-overflow: ellipsis;\r\n    display: flex;\r\n    align-items: center;\r\n    color: #b3b3b3;\r\n    //padding: 16px 10px;\r\n    height: 56px;\r\n    line-height: 56px;\r\n    transition: color @transition-time;\r\n\r\n    &-icon {\r\n      width: 18px;\r\n      height: 18px;\r\n      display: inline-flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      margin-right: 10px;\r\n    }\r\n  }\r\n\r\n  .h-menu-li:hover {\r\n    .second-menu {\r\n      display: block !important;\r\n    }\r\n  }\r\n\r\n  .second-menu {\r\n    box-shadow: 0 1px 4px rgba(0, 21, 41, 0.08);\r\n\r\n    position: absolute;\r\n    display: none;\r\n    //left: 100%;\r\n    left: 150px;\r\n    z-index: 1055;\r\n    top: 0;\r\n    border-left: 1px solid rgba(239, 236, 236, 0.89);\r\n\r\n    .second-menu-wrap {\r\n      overflow: hidden;\r\n      display: table;\r\n      padding: 20px 0;\r\n      background-color: #3d74ff;\r\n      color: #FFF;\r\n      white-space: nowrap;\r\n      position: relative;\r\n      z-index: 1055;\r\n    }\r\n\r\n    .second-menu-item {\r\n      display: table-cell;\r\n      line-height: 30px;\r\n      height: 100%;\r\n      border-right: 1px solid rgba(239, 236, 236, 0.89);\r\n\r\n      .second-menu-title {\r\n        color: #dddbe3;\r\n        padding: 0 40px;\r\n      }\r\n\r\n      li {\r\n        display: block;\r\n        list-style: none;\r\n        cursor: pointer;\r\n        vertical-align: top;\r\n        padding: 13px 40px;\r\n        &:hover{\r\n          color: @yellow-color;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  &:hover{\r\n    .h-menu-li-selected{\r\n      color: #b3b3b3;\r\n      background-color: transparent;\r\n    }\r\n  }\r\n\r\n  .h-menu-show-icon{\r\n    i{\r\n      font-size: 18px !important;\r\n    }\r\n  }\r\n\r\n  .h-menu-first-level:hover{\r\n\r\n    background-color: #3d74ff;\r\n    .h-menu-show-desc{\r\n      color: @white-color;\r\n    }\r\n    .h-menu-show-icon{\r\n      color: @white-color;\r\n    }\r\n  }\r\n\r\n\r\n  .h-menu-li-selected {\r\n    color: @white-color;\r\n    background: #3d74ff;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;AAmCA,OAAOA,OAAM,MAAO,oBAAoB;AACxC,SAAQC,QAAQ,EAACC,YAAY,QAAO,MAAM;AAC1C,SAAQC,IAAI,QAAO,UAAU;;AAE7B;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAK,EAAE;IACLC,KAAK,EAAEC;EACT,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGP,QAAQ,CAAC,CAAC,gBAAgB,EAAE,OAAO,EAAE,YAAY,EAAE,MAAM,CAAC,CAAC;IAC9DQ,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,cAAa,GAAI,UAAS,GAAI,UAAU;IACtD;EACF,CAAC;EACDC,KAAK,EAAE;IACLC,UAAUA,CAACC,GAAG,EAAE;MACdC,QAAQ,CAACC,IAAG,GAAIF,GAAG;MACnBG,YAAY,CAACC,OAAO,CAAC,YAAY,EAAEC,IAAI,CAACC,SAAS,CAAChB,IAAI,CAAC,IAAI,CAACiB,IAAI,CAACC,IAAI,CAACC,KAAI,IAAKA,KAAK,CAACC,GAAE,KAAMV,GAAG,CAAC,EAAE,CAAC,KAAK,EAAE,OAAO,CAAC,CAAC,CAAC;IACvH;EACF,CAAC;EACDW,OAAO,EAAE;IACP,GAAGtB,YAAY,CAAC,CAAC,SAAS,EAAE,WAAW,CAAC,CAAC;IACzCuB,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,eAAc,KAAM,IAAI,CAACd,UAAU,EAAE;QACvC,IAAI,IAAI,CAACe,KAAK,EAAE;UACd,IAAIN,IAAG,GAAI,IAAI,CAACM,KAAK,CAACN,IAAI,CAACR,GAAE,IAAK;YAChC,IAAIA,GAAG,CAACU,GAAE,KAAM,IAAI,CAACX,UAAU,EAAE;cAC/B,OAAO,IAAI;YACb,OAAO,IAAIC,GAAG,CAACe,QAAQ,EAAE;cACvB,KAAK,MAAMC,EAAC,IAAKhB,GAAG,CAACe,QAAQ,EAAE;gBAC7B,IAAIC,EAAE,CAACN,GAAE,KAAM,IAAI,CAACX,UAAU,EAAE;kBAC9B,OAAO,IAAI;gBACb,OAAO,IAAIiB,EAAE,CAACD,QAAQ,EAAE;kBACtB,KAAK,MAAME,EAAC,IAAKD,EAAE,CAACD,QAAQ,EAAE;oBAC5B,IAAIE,EA