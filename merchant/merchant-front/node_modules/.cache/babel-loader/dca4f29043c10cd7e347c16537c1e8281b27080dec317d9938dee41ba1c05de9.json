\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CustomersForm from \"./CustomerForm.vue\";\r\nimport Customers from \"@js/api/Customers\";\r\nimport {confirm, message} from \"heyui.ext\";\r\nimport {layer} from \"@layui/layer-vue\";\r\nimport {h} from \"vue\";\r\nimport CustomersCategoryForm from \"@components/group/basics/CustomerCategoryForm.vue\";\r\n\r\n/**\r\n * @功能描述: 客户管理\r\n * @创建时间: 2023年08月08日\r\n * @公司官网: www.fenxi365.com\r\n * @公司信息: 纷析云（杭州）科技有限公司\r\n * @公司介绍: 专注于财务相关软件开发, 企业会计自动化解决方案\r\n */\r\nexport default {\r\n  name: \"CustomersList\",\r\n  components: {CustomersForm},\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      params: {\r\n        name: null,\r\n      },\r\n      dataList: [],\r\n      pagination: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0\r\n      },\r\n    }\r\n  },\r\n  computed: {\r\n    queryParams() {\r\n      return Object.assign(this.params, {\r\n        page: this.pagination.page,\r\n        pageSize: this.pagination.pageSize,})\r\n    }\r\n  },\r\n  methods: {\r\n    addCategoryForm(entity) {\r\n      let type = 0;\r\n      let layerId = layer.open({\r\n        title: \"客户分类\",\r\n        shadeClose: false,\r\n        closeBtn: false,\r\n        area: ['400px', '230px'],\r\n        content: h(CustomersCategoryForm, {\r\n          entity, type,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    showForm(entity) {\r\n      let layerId = layer.open({\r\n        title: \"客户信息\",\r\n        shadeClose: false,\r\n        closeBtn: false,\r\n        area: ['700px', '500px'],\r\n        content: h(CustomersForm, {\r\n          entity,\r\n          onClose: () => {\r\n            layer.close(layerId);\r\n          },\r\n          onSuccess: () => {\r\n            this.doSearch();\r\n            layer.close(layerId);\r\n          }\r\n        })\r\n      });\r\n    },\r\n    loadList() {\r\n      this.loading = true;\r\n      Customers.list(this.queryParams).then(({data: {results, total}}) => {\r\n        this.dataList = results || [];\r\n        this.pagination.total = total;\r\n      }).finally(() => this.loading = false);\r\n    },\r\n    doSearch() {\r\n      this.pagination.page = 1;\r\n      this.loadList();\r\n    },\r\n    doRemove(row) {\r\n      confirm({\r\n        title: \"系统提示\",\r\n        content: `确认删除用户：${row.name}?`,\r\n        onConfirm: () => {\r\n          Customers.remove(row.id).then(() => {\r\n            message(\"删除成功~\");\r\n            this.loadList();\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.loadList();\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA0DA,OAAOA,aAAY,MAAO,oBAAoB;AAC9C,OAAOC,SAAQ,MAAO,mBAAmB;AACzC,SAAQC,OAAO,EAAEC,OAAO,QAAO,WAAW;AAC1C,SAAQC,KAAK,QAAO,kBAAkB;AACtC,SAAQC,CAAC,QAAO,KAAK;AACrB,OAAOC,qBAAoB,MAAO,mDAAmD;;AAErF;;;;;;;AAOA,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IAACR;EAAa,CAAC;EAC3BS,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE;QACNJ,IAAI,EAAE;MACR,CAAC;MACDK,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;QACVC,IAAI,EAAE,CAAC;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT;IACF;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAOC,MAAM,CAACC,MAAM,CAAC,IAAI,CAACT,MAAM,EAAE;QAChCG,IAAI,EAAE,IAAI,CAACD,UAAU,CAACC,IAAI;QAC1BO,QAAQ,EAAE,IAAI,CAACR,UAAU,CAACQ;MAAS,CAAC;IACxC;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAACC,MAAM,EAAE;MACtB,IAAIC,IAAG,GAAI,CAAC;MACZ,IAAIC,OAAM,GAAItB,KAAK,CAACuB,IAAI,CAAC;QACvBC,KAAK,EAAE,MAAM;QACbC,UAAU,EAAE,KAAK;QACjBC,QAAQ,EAAE,KAAK;QACfC,IAAI,EAAE,CAAC,OAAO,EAAE,OAAO,CAAC;QACxBC,OAAO,EAAE3B,CAAC,CAACC,qBAAqB,EAAE;UAChCkB,MAAM;UAAEC,IAAI;UACZQ,OAAO,EAAEA,CAAA,KAAM;YACb7B,KAAK,CAAC8B,KAAK,CAACR,OAAO,CAAC;UACtB,CAAC;UACDS,SAAS,EAAEA,CAAA,KAAM;YACf,IAAI,CAACC,QAAQ,CAAC,CAAC;YACfhC,KAAK,CAAC8B,KAAK,CAACR,OAAO,CAAC;UACtB;QACF,CAAC;MACH,CAAC,CAAC;IACJ,CAAC;IA