eryParams).then(({\n        data: {\n          results,\n          total\n        }\n      }) => {\n        this.dataList = results || [];\n        this.pagination.total = total;\n      }).finally(() => this.loading = false);\n    },\n    doSearch() {\n      this.pagination.page = 1;\n      this.loadList();\n    },\n    doRemove(row) {\n      confirm({\n        title: \"系统提示\",\n        content: `确认删除用户：${row.name}?`,\n        onConfirm: () => {\n          Customers.remove(row.id).then(() => {\n            message(\"删除成功~\");\n            this.loadList();\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.loadList();\n  }\n};","map":{"version":3,"names":["CustomersForm","Customers","confirm","message","layer","h","CustomersCategoryForm","name","components","data","loading","params","dataList","pagination","page","size","total","computed","queryParams","Object","assign","pageSize","methods","addCategoryForm","entity","type","layerId","open","title","shadeClose","closeBtn","area","content","onClose","close","onSuccess","doSearch","showForm","loadList","list","then","results","finally","doRemove","row","onConfirm","remove","id","created"],"sources":["D:\\DevWorks\\jxc\\merchant\\merchant-front\\src\\views\\basic\\CustomerList.vue"],"sourcesContent":["<template><div class=\"frame-page flex flex-column\">\r\n  <vxe-toolbar>\r\n    <template #buttons>\r\n      <Search v-model.trim=\"params.filter\" search-button-theme=\"h-btn-default\"\r\n              show-search-button class=\"w-260px\"\r\n              placeholder=\"请输入客户名称\" @search=\"doSearch\">\r\n        <i class=\"h-icon-search\"/>\r\n      </Search>\r\n    </template>\r\n    <template #tools>\r\n      <Button @click=\"showForm()\" color=\"primary\">新 增</Button>\r\n      <Button @click=\"addCategoryForm()\" color=\"primary\">新增分类</Button>\r\n    </template>\r\n  </vxe-toolbar>\r\n  <div class=\"flex1\">\r\n        <vxe-table row-id=\"id\"\r\n                   ref=\"table\"\r\n             